Clase Animal #superclase
    Atributos
        Edad: entero
        Especie: entero
    Métodos:
        alimentarse()
        moverse()

Clase Mamífero(hereda de Animal) #subclase
    Atributos
        Pelaje
    Métodos
        Lactancia()

Clase Ovíparo (hereda de Animal)
    Atributos

    Métodos
        PoneHuevos

Clase Gato (Hereda de Mamífero) # subclase
    Atributos:
        Color: cadena
    Métodos:
        Maullar()

Clase Pollo(hereda de Ovíparo) # subclase
    Atributos
        Color: cadena
    Métodos:
        HAcerSonido()

Clase Ornitorrinco(herena de mamífero y Ovíparo) # subclase
    Atributos
        Tamaño
    Métodos:
        Nadar()
